<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Index", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("CreateSession", "CreateSession", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("CreateSession1", "CreateSession1", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("ReadSession", "ReadSession", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("ReadSession1", "ReadSession1", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("ClearSession", "ClearSession", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("RemoveSessionItem", "RemoveSessionItem", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("RemoveSessionItem1", "RemoveSessionItem1", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("RemoveAll", "RemoveAll", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Abandon", "Abandon", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("ReadSessioId", "ReadSessioId", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>